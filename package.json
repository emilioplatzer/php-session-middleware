  "bugs": {
    "url": "https://github.com/emilioplatzer/php-session-middleware/issues"
  },
  "homepage": "https://github.com/emilioplatzer/php-session-middleware",
  "dependencies": {
    "php-unserialize": "0.0.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "express": "^4.14.0",
    "supertest": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "istanbul": "^0.4.5"
  }
